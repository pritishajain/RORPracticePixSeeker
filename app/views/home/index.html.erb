
 <%= render 'home/form' %> 
 <br>
 <% if @photos.present? %>
  <h2 style="text-align:center">Search Results for <%= @category_name %></h2>
  <div class="images">
    <% @photos.each do |photo| %>
      <img src="<%= photo['src']['medium'] %>" alt="<%= photo['url'] %>" width="200px" height="200px">
    <% end %>
  </div>
<% end %>




